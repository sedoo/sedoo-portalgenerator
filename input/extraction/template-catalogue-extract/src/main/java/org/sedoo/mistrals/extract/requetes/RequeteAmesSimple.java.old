package org.sedoo.mistrals.extract.requetes;

import java.sql.Timestamp;
import java.util.List;

import org.sedoo.mistrals.extract.utils.Zone;

public class RequeteAmesSimple {
	
	int datsId;
	int placeId;
	List<Integer> varIds;
	
	List<Integer> insDatsIds;

	Timestamp dateMin = null;
	Timestamp dateMax = null;
	
	private Zone zone;
	
	private String compression;
			
	public RequeteAmesSimple(int datsId, int placeId, List<Integer> varIds, List<Integer> insDatsIds, Timestamp dateMin, Timestamp dateMax, Zone zone, String compression) {
		super();
		this.datsId = datsId;
		this.placeId = placeId;
		this.varIds = varIds;
		this.insDatsIds = insDatsIds;
		this.dateMin = dateMin;
		this.dateMax = dateMax;
		this.zone = zone;
		this.compression = compression;
	}

	public int getDatsId() {
		return datsId;
	}

	public int getPlaceId() {
		return placeId;
	}

	public List<Integer> getInsDatsIds() {
		return insDatsIds;
	}
	
	public List<Integer> getVarIds() {
		return varIds;
	}

	public Timestamp getDateMin() {
		return dateMin;
	}

	public Timestamp getDateMax() {
		return dateMax;
	}

	public Zone getZone() {
		return zone;
	}
	
	public String getCompression() {
		return compression;
	}
	
}
